<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/holo_blue_dark"
    tools:context="com.example.zybang.myapplication.com.example.zybang.fragment.EditAnimFragment"
    >

<com.example.zybang.myapplication.com.example.zybang.customview.BiuEditText
    android:id="@+id/biu1"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="100dp"
    android:hint="biu,biu,发射1号"
    android:textColor="@android:color/white"
    app:biu_duration="800"
    app:biu_text_color="@android:color/white"
    app:biu_text_scale="1.5"
    app:biu_text_start_size="12sp"
    app:biu_type="flydown"
    />
    <com.example.zybang.myapplication.com.example.zybang.customview.BiuEditText
        android:id="@+id/biu_text_2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="biu,biu,发射2号"
        android:textColor="@android:color/white"
        app:biu_duration="800"
        app:biu_text_color="@android:color/white"
        app:biu_text_scale="1.5"
        app:biu_text_start_size="12sp"
        app:biu_type="flyup"
        />
    <FrameLayout
        android:layout_below="@id/biu1"
        android:layout_marginTop="20dp"
        android:id="@+id/fragment_container"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        />
</RelativeLayout>
